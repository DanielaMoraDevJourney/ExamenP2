<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AppNotesDanielaMoraMAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:AppNotesDanielaMoraMAUI.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="AppNotesDanielaMoraMAUI">

    <TabBar>
        <ShellContent
            Title="Notes_DMO"
            ContentTemplate="{DataTemplate views:AllNotesPage_DMO}"
            Icon="{OnPlatform 'icon_notes_dm.png', iOS='icon_notes_ios_dm.png', MacCatalyst='icon_notes_ios_dm.png'}" />

        <ShellContent
            Title="About_DMO"
            ContentTemplate="{DataTemplate views:AboutPage_DMO}"
            Icon="{OnPlatform 'icon_about_dm.png', iOS='icon_about_ios_dm.png', MacCatalyst='icon_about_ios_dm.png'}" />
    </TabBar>

</Shell>
